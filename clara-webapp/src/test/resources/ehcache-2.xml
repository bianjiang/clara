<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ehcache>
	<diskStore path="java.io.tmpdir/clara-webapp-ehcache/" />

    <defaultCache
                maxElementsInMemory="500000"
                eternal="false"
                overflowToDisk="false"
                memoryStoreEvictionPolicy="LFU"
                timeToIdleSeconds="30" timeToLiveSeconds="60" />



    <cache name="edu.uams.clara.common.domain.UploadedFile" maxElementsInMemory="500000"
		eternal="true" overflowToDisk="false"
		memoryStoreEvictionPolicy="LFU"
		timeToIdleSeconds="300" timeToLiveSeconds="600"
    diskPersistent="true" diskExpiryThreadIntervalSeconds="300" />

     <cache name="edu.uams.clara.webapp.common.domain.usercontext.User" maxElementsInMemory="500000"
		eternal="true" overflowToDisk="false"
		memoryStoreEvictionPolicy="LFU"
		timeToIdleSeconds="300" timeToLiveSeconds="600"
    diskPersistent="true" diskExpiryThreadIntervalSeconds="300" />

     <cache name="edu.uams.clara.webapp.common.domain.usercontext.Person" maxElementsInMemory="500000"
		eternal="true" overflowToDisk="false"
		memoryStoreEvictionPolicy="LFU"
		timeToIdleSeconds="300" timeToLiveSeconds="600"
    diskPersistent="true" diskExpiryThreadIntervalSeconds="300" />

     <cache name="edu.uams.clara.webapp.common.domain.usercontext.Authority" maxElementsInMemory="500000"
		eternal="true" overflowToDisk="false"
		memoryStoreEvictionPolicy="LFU"
		timeToIdleSeconds="300" timeToLiveSeconds="600"
    diskPersistent="true" diskExpiryThreadIntervalSeconds="300" />

    <cache name="edu.uams.clara.webapp.protocol.domain.Protocol" maxElementsInMemory="500000"
		eternal="false" overflowToDisk="false"
		memoryStoreEvictionPolicy="LFU"
		timeToIdleSeconds="300" timeToLiveSeconds="600"
    diskPersistent="true" diskExpiryThreadIntervalSeconds="300" />



</ehcache>