	<bean id="maintainenceDao" class="edu.uams.clara.webapp.maintainence.dao.MaintainenceDao"/>
	
	<bean id="xmlHandler" class="edu.uams.clara.core.util.xml.impl.XmlHandlerImpl" />
	
	<bean id="contractFormDao"
